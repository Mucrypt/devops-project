apiVersion: v1
kind: Secret
metadata:
  name: mongodb-secret
  namespace: nexusai
type: Opaque
data:
  # Base64 encoded values - REPLACE THESE WITH YOUR OWN!
  # To encode: echo -n 'your-value' | base64
  # Example: echo -n 'mynewpassword' | base64
  mongodb-root-username: REPLACE_WITH_BASE64_USERNAME  # e.g., bmV4dXNhaQ== (nexusai)
  mongodb-root-password: REPLACE_WITH_BASE64_PASSWORD  # e.g., bmV4dXNhaTEyMw== (nexusai123)
